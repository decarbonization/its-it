{
  "name": "its-it",
  "type": "commonjs",
  "version": "0.1.0",
  "description": "Iterator and collection tools",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./fill": {
      "types": "./dist/lib/fill.d.ts",
      "default": "./dist/lib/fill.js"
    },
    "./range": {
      "types": "./dist/lib/range.d.ts",
      "default": "./dist/lib/range.js"
    },
    "./repeat": {
      "types": "./dist/lib/repeat.d.ts",
      "default": "./dist/lib/repeat.js"
    }
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "npx rimraf dist",
    "build": "npx tsc",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decarbonization/its-it.git"
  },
  "keywords": [
    "iterators",
    "collections",
    "arrays"
  ],
  "author": "Peter Kevin Contreras",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decarbonization/its-it/issues"
  },
  "homepage": "https://github.com/decarbonization/its-it#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0",
    "npm": ">=10.5.0"
  }
}
